{
    "name": "desu-project/chanbooru-interface",
    "description": "A PHP interfaces used to create PHP SDKs for imageboards like Danbooru, Konachan and others.",
    "type": "library",
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.11"
    },
    "authors": [
        {
            "name": "Petr Luzhin",
            "email": "lithx@yandex.com"
        }
    ],
    "require": {},
    "autoload": {
        "psr-4": {
            "DesuProject\\ChanbooruInterface\\": "src/"
        }
    },
    "scripts": {
        "fix-cs": [
            "vendor/bin/php-cs-fixer fix --config=.php_cs.dist --verbose --show-progress=dots"
        ],
        "lint": [
            "vendor/bin/php-cs-fixer fix --config=.php_cs.dist --verbose --show-progress=dots --dry-run"
        ]
    }
}
